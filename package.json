{
  "name": "infoschool-root",
  "version": "1.0.0",
  "description": "Orquestrador do projeto InfoSchool - Executa Front e Back simultaneamente",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:front-end\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:front-end": "npm run dev --prefix front-end",
    "install:all": "npm install --prefix backend && npm install --prefix front-end",
    "postinstall": "npm run install:all"
  },
  "author": "UnB-MDS",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@google-cloud/bigquery": "^8.1.1",
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^0.2.0",
    "@langchain/google-genai": "^2.0.0",
    "@langchain/openai": "^0.1.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "elasticlunr": "^0.9.5",
    "fastify": "^5.6.1",
    "hnswlib-node": "^3.0.0",
    "nodemon": "^3.1.10"
  },
  "devDependencies": {
    "pino-pretty": "^13.1.2"
  }
}